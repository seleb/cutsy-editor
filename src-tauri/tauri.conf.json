{
  "build": {
    "beforeDevCommand": "npm run tauri icon src/public/app-icon.png && npm run start:ui",
    "beforeBuildCommand": "npm run tauri icon src/public/app-icon.png && npm run build:ui",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "cutsy",
    "version": "../package.json"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "all": false,
        "save": true,
        "open": true,
        "message": true,
        "ask": true,
        "confirm": true
      },
      "shell": {
        "all": false,
        "open": "^((mailto:\\w+)|(tel:\\w+)|(https?://\\w+)|(file:///\\w+)).+"
      },
      "window": {
        "all": false,
        "setFullscreen": true,
        "setTitle": true,
        "close": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": false,
        "readDir": true,
        "scope": [
          "$VIDEO",
          "$VIDEO/**/*"
        ]
      },
      "protocol": {
        "all": false,
        "asset": true,
        "assetScope": [
          "$VIDEO",
          "$VIDEO/**/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "site.seans.cutsy",
      "targets": [
        "msi",
        "app",
        "appimage"
      ],
      "publisher": "Sean S. LeBlanc",
      "copyright": "Sean S. LeBlanc",
      "category": "GraphicsAndDesign",
      "shortDescription": "a lil video clip editor"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "cutsy",
        "width": 800,
        "height": 600
      }
    ]
  }
}
